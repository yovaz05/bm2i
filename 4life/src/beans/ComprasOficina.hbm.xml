<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

<class name="beans.ComprasOficina" table="COMPRASO">
    <id name="id_Compras"  column="ID_COMPRAS">
        <generator class="native"/>
    </id>  
    
    <property name="fecha" column="FECHA"/>
    <property name="descripcion" column="DESCRIPCION"/>
    <property name="valor" column="VALOR"/>
    
        
   <property name="gmt" type="java.lang.Boolean"  >
   <column name="GMT" sql-type="tinyint(1)" not-null="true"/>
   </property>
   
    <many-to-one name="horario" column="ID_HORARIO"/>
    
    
</class>
</hibernate-mapping>
