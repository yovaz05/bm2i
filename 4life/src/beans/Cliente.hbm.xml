<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

<class name="beans.Cliente" table="CLIENTE">
    
    <id name="id_cliente"  column="ID_CLIENTE">
        <generator class="native"/>        
    </id>            
    <property name="nombre" column="nombre"/>
    <property name="nombres" column="nombres"/>
    <property name="apellidos" column="apellido"/>
    <property name="cedulaRuc" column="cedularuc"/>
    <property name="codigoCliente" column="codigocliente"/>
    <property name="email" column="email"/>
               
</class>
</hibernate-mapping>
